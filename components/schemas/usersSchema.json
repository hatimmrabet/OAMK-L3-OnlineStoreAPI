{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "hatimmrabet2",
            "password": "5454656",
            "firstName": "Hatim",
            "lastName": "M'rabet El Khomssi",
            "dateOfBirth": "2000-06-30",
            "email": "hatimmrabet2@gmail.com",
            "gender": "Male",
            "phoneNumber": "+33618256737",
            "city": "Helsinki",
            "contryCode": "FI"
        }
    ],
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "dateOfBirth",
        "email",
        "gender",
        "phoneNumber",
        "city",
        "contryCode"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "hatimmrabet2"
            ],
            "minLength": 1
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "5454656"
            ],
            "minLength": 5
        },
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "The firstName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Hatim"
            ],
            "minLength": 1
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "The lastName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "M'rabet El Khomssi"
            ],
            "minLength": 1
        },
        "dateOfBirth": {
            "$id": "#/properties/dateOfBirth",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "2000-06-30"
            ],
            "title": "The dateOfBirth schema",
            "type": "string",
            "pattern": "^[0-9]{4}-([0][1-9]|[1][0-2])-([0][1-9]|[1-2][0-9]|[3][0-1])$",
            "minLength": 1
        },
        "email": {
            "$id": "#/properties/email",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "hatimmrabet2@gmail.com"
            ],
            "title": "The email schema",
            "type": "string",
            "pattern": "^[a-zA-Z0-9]+@[a-zA-Z0-9]+[.][a-zA-Z0-9]+$",
            "minLength": 1
        },
        "gender": {
            "$id": "#/properties/gender",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "male"
            ],
            "title": "The gender schema",
            "enum": [
                "male",
                "female"
            ],
            "type": "string",
            "minLength": 1
        },
        "phoneNumber": {
            "$id": "#/properties/phoneNumber",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "+33618256737"
            ],
            "title": "The phoneNumber schema",
            "pattern": "^[+][0-9]{10,14}$",
            "maxLength": 15,
            "minLength": 8,
            "type": "string"
        },
        "city": {
            "$id": "#/properties/city",
            "type": "string",
            "title": "The city schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Helsinki"
            ],
            "minLength": 1
        },
        "contryCode": {
            "$id": "#/properties/contryCode",
            "default": "",
            "description": "An explanation about the purpose of this instance.",
            "examples": [
                "FI"
            ],
            "maxLength": 2,
            "minLength": 2,
            "title": "The contryCode schema",
            "type": "string"
        }
    },
    "additionalProperties": false
}